<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>1.0.2</Version>
    <Description>An easy to use, simple yet feature-rich open-source framework for writing middlewares in the form of a low resource footnote Windows services or Linux Daemons.
Takes care of craceful multi-threaded service granular scheduling, shutdowns, restarts, error recovery.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/H7O/Com.H.Threading</PackageProjectUrl>
    <RepositoryUrl>https://github.com/H7O/Com.H.Threading</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Multi-threading,Concurrency,Windows Service,Linux Daemon</PackageTags>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Hussein Al Bayati</Authors>
    <AnalysisLevel>latest</AnalysisLevel>
    <EnforceCodeStyleInBuild>false</EnforceCodeStyleInBuild>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Com.H">
      <HintPath>..\..\Com.H\Com.H\bin\Release\net5.0\Com.H.dll</HintPath>
    </Reference>
    <Reference Include="Com.H.Data">
      <HintPath>..\..\Com.H.Data\Com.H.Data\bin\Release\net5.0\Com.H.Data.dll</HintPath>
    </Reference>
    <Reference Include="Com.H.Threading">
      <HintPath>..\..\Com.H.Threading\Com.H.Threading\bin\Release\net5.0\Com.H.Threading.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
